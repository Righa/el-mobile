<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.e_learning_mobile">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_e_learning"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_e_learning_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ExploreActivity" />
        <activity android:name=".ReviewActivity" />
        <activity android:name=".ExamViewActivity" />
        <activity android:name=".ForumViewActivity" />
        <activity android:name=".CourseViewActivity" />
        <activity android:name=".AssessmentActivity" />
        <activity android:name=".CoursesActivity" />
        <activity android:name=".ForumsActivity" />
        <activity android:name=".ExamsActivity" />
        <activity android:name=".DashboardActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>